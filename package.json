{
  "name": "tesseract",
  "version": "1.0.0",
  "description": "A gpgpu compute library for javascript",
  "main": "bin/index.js",
  "scripts": {
    "build"     : "tsc-bundle ./src/index.ts  dst/tesseract.js --removeComments --exportAs tesseract",
